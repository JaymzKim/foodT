				<IMAGE layout="194,50,333,134" defaultImg="res/main_pickup_label.png" />
				<LABEL layout="0,200,720,60" text="쫀득 따뜻한 감자전" fontSize="50" minFontSize="-1" textAlign="center" fontStyle="bold" textColor="#ffffff" textShadow="#90000000" />
				<LABEL layout="0,277,720,35" text="감자전" fontSize="20" minFontSize="-1" textAlign="center" fontStyle="bold" textColor="#ffffff" textShadow="#90000000" />
				<LABEL layout="0,317,720,35" text="Lets make it!" fontSize="25" minFontSize="-1" textAlign="center" fontStyle="bold" textColor="#ffffff" textShadow="#90000000" />
			</WINDOW>
			<WINDOW layout="720,123" defaultImg="res/notice_bottom_bg.png" align="relative">
				<IMAGE layout="32,56,296,63" defaultImg="res/main_title_theme.png" />
				<IMAGE layout="570,71,138,39" defaultImg="res/main_seemore.png" />
			</WINDOW>
			<WINDOW layout="720,466" align="relative" defaultImg="res/foodtalk.png" />
			<WINDOW layout="720,466" align="relative" defaultImg="res/detail1.png" />
