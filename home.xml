			</WINDOW>

			<WINDOW layout="720,466" align="relative" defaultImg="res/detail3.png" margin="0,20,0,0">
				<IMAGE layout="31,296,107,44" defaultImg="res/feed_label_recipe.png" />
				<LABEL layout="25,353,450,45" text="버섯레몬크림파스타" fontSize="40" minFontSize="-1" textAlign="vCenter" fontStyle="bold" textColor="#ffffff" textShadow="#90000000" />
				<LABEL layout="25,406,450,40" text="by ESSEN" fontSize="30" minFontSize="-1" textAlign="vCenter" fontStyle="bold" textColor="#ffffff" textShadow="#90000000" />
				<BUTTON layout="596,336,105,105" defaultImg="res/btn_unlike_count_normal.png" onClick="function.faviCheck" />
